{
  "name": "undraw-ui",
  "private": false,
  "version": "1.2.3",
  "type": "module",
  "author": "readpage <f1dao@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/undraw/undraw-ui.git"
  },
  "keywords": [
    "undraw-ui",
    "element-plus",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "homepage": "https://readpage.github.io/undraw-ui/",
  "license": "MIT",
  "main": "es/index.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "es/index.d.ts",
  "style": "dist/style.css",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:es && npm run build:umd",
    "build:es": "vite build --emptyOutDir --config ./scripts/build.es.ts && node ./scripts/element.js",
    "build:umd": "vite build --emptyOutDir --config ./scripts/build.umd.ts && node ./scripts/remove.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.6.0",
    "vue": "^3.3.4",
    "vue-dompurify-html": "5.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "sass": "^1.49.7",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vue-tsc": "^1.8.5"
  }
}
